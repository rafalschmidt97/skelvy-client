{
  "name": "skelvy",
  "description": "Mobile app for meetings over your favorite activities.",
  "version": "2.3.1",
  "private": true,
  "author": "RafaÅ‚ Schmidt",
  "homepage": "https://skelvy.com/",
  "engines": {
    "node": ">= 10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rafalschmidt97/skelvy-client"
  },
  "config": {
    "hostname": "REPLACE_WITH_MACHINE_IP"
  },
  "scripts": {
    "start": "ionic lab",
    "serve": "ionic serve",
    "lint": "npm run lint:ts && npm run lint:scss && npm run lint:html",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:html": "prettier --list-different 'src/**/*.html' --parser angular",
    "format": "npm run format:ts && npm run format:scss && npm run format:html",
    "format:ts": "prettier --write 'src/**/*.ts' --parser typescript",
    "format:scss": "prettier --write 'src/**/*.scss' --parser scss",
    "format:html": "prettier --write 'src/**/*.html' --parser angular",
    "ios": "npm run ios:device",
    "ios:device": "ionic cordova run ios -l --device --no-native-run --address=0.0.0.0",
    "ios:emulator": "ionic cordova run ios -l --emulator --no-native-run --address=0.0.0.0",
    "ios:prepare": "ionic cordova prepare ios && cd platforms/ios && pod install && cd .. && cd ..",
    "ios:build": "ionic cordova build ios --prod --release --packageType=\"app-store\"",
    "android": "npm run android:device",
    "android:device": "ionic cordova run android -l --device --no-native-run --address=0.0.0.0",
    "android:emulator": "ionic cordova run android -l --emulator --no-native-run --address=0.0.0.0",
    "android:prepare": "ionic cordova prepare android",
    "android:build": "ionic cordova build android --prod --release",
    "translate": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format json --marker _",
    "redux": "npx remotedev --hostname=$hostname --port=8000"
  },
