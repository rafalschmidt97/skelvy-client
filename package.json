{
  "name": "skelvy",
  "description": "Mobile app for meetings over beer or coffee",
  "version": "0.0.1",
  "private": true,
  "author": "RafaÅ‚ Schmidt",
  "homepage": "https://rafalschmidt.com/",
  "engines": {
    "node": ">= 10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rafalschmidt97/skelvy-client"
  },
  "scripts": {
    "start": "ionic lab",
    "serve": "ionic serve",
    "lint": "npm run lint:ts && npm run lint:scss && npm run lint:html",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint --syntax scss '**/**.scss'",
    "lint:html": "prettier --list-different '**/**/*.html' --parser angular",
    "format": "npm run format:ts && npm run format:scss && npm run format:html",
    "format:ts": "prettier --write '**/**/*.ts' --parser typescript",
    "format:scss": "prettier --write '**/**/*.scss' --parser scss",
    "format:html": "prettier --write '**/**/*.html' --parser angular",
    "ios:prepare": "ionic cordova prepare ios",
    "ios": "ionic cordova run ios -l  -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "ios:emulate": "ionic cordova emulate ios -l --target=\"iPhone-X\"  -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "build:ios": "ionic cordova build ios --prod",
    "translate": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format json --marker _"
  },
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/cdk": "^7.2.2",
    "@angular/common": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/email-composer": "^5.0.0",
    "@ionic-native/keyboard": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.0.0",
    "@ionic/storage": "^2.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "blob-util": "^2.0.2",
    "bootstrap": "^4.2.1",
    "cordova-ios": "4.5.5",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-email-composer": "^0.9.2",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^2.3.2",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-sqlite-storage": "^2.6.0",
    "core-js": "^2.6.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^3.2.0",
    "ngx-image-cropper": "^1.3.8",
    "primeng": "^7.0.5",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.12.3",
    "@angular-devkit/build-angular": "~0.12.3",
    "@angular-devkit/core": "~7.2.3",
    "@angular-devkit/schematics": "~7.2.3",
    "@angular/cli": "~7.2.3",
    "@angular/compiler": "~7.2.2",
    "@angular/compiler-cli": "~7.2.2",
    "@angular/language-service": "~7.2.2",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ionic/angular-toolkit": "~1.2.0",
    "@ionic/lab": "^1.0.19",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "~3.3.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "popper.js": "^1.14.6",
    "prettier": "^1.16.1",
    "protractor": "~5.4.0",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-prettier": "^1.0.6",
    "ts-node": "~7.0.0",
    "tslint": "^5.12.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.1.6"
  },
  "cordova": {
    "plugins": {
      "cordova-sqlite-storage": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-email-composer": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {}
    },
    "platforms": [
      "ios"
    ]
  }
}
