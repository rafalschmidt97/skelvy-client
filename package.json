{
  "name": "skelvy",
  "description": "Mobile app for meetings over your favorite drinks.",
  "version": "1.0.3",
  "private": true,
  "author": "RafaÅ‚ Schmidt",
  "homepage": "https://skelvy.com/",
  "engines": {
    "node": ">= 10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rafalschmidt97/skelvy-client"
  },
  "config": {
    "hostname": "REPLACE_WITH_MACHINE_IP"
  },
  "scripts": {
    "start": "ionic lab",
    "serve": "ionic serve",
    "lint": "npm run lint:ts && npm run lint:scss && npm run lint:html",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint --syntax scss 'src/**/*.scss'",
    "lint:html": "prettier --list-different 'src/**/*.html' --parser angular",
    "format": "npm run format:ts && npm run format:scss && npm run format:html",
    "format:ts": "prettier --write 'src/**/*.ts' --parser typescript",
    "format:scss": "prettier --write 'src/**/*.scss' --parser scss",
    "format:html": "prettier --write 'src/**/*.html' --parser angular",
    "ios": "ionic cordova run ios -l  -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "ios:emulate": "ionic cordova emulate ios -l --target=\"iPhone-X\"  -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "ios:prepare": "ionic cordova prepare ios && cd platforms/ios && pod install && cd .. && cd ..",
    "ios:build": "ionic cordova build ios --prod --release --packageType=\"app-store\" -- --buildFlag=\"-UseModernBuildSystem=0\"",
    "android": "ionic cordova run android -l",
    "android:emulate": "ionic cordova emulate android -l",
    "android:prepare": "ionic cordova prepare android",
    "android:build": "ionic cordova build android --prod --release",
    "translate": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format json --marker _",
    "redux": "npx remotedev --hostname=$hostname --port=8000"
  },
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@aspnet/signalr": "^1.1.4",
    "@auth0/angular-jwt": "^2.1.1",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@ionic-native/app-rate": "^5.8.0",
    "@ionic-native/camera": "^5.9.0",
    "@ionic-native/core": "^5.8.0",
    "@ionic-native/device": "^5.8.0",
    "@ionic-native/email-composer": "^5.8.0",
    "@ionic-native/facebook": "^5.8.0",
    "@ionic-native/geolocation": "^5.8.0",
    "@ionic-native/google-plus": "^5.8.0",
    "@ionic-native/in-app-browser": "^5.8.0",
    "@ionic-native/push": "^5.8.0",
    "@ionic-native/social-sharing": "^5.8.0",
    "@ionic-native/splash-screen": "^5.8.0",
    "@ionic-native/status-bar": "^5.8.0",
    "@ionic/angular": "^4.6.0",
    "@ionic/storage": "^2.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngxs/store": "^3.4.3",
    "blob-util": "^2.0.2",
    "bootstrap": "^4.3.1",
    "cordova-android": "7.1.4",
    "cordova-ios": "4.5.5",
    "cordova-plugin-apprate": "^1.4.0",
    "cordova-plugin-camera": "4.0.3",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-dialogs": "^2.0.2",
    "cordova-plugin-email-composer": "^0.9.2",
    "cordova-plugin-facebook4": "^4.2.1",
    "cordova-plugin-geolocation": "^4.0.1",
    "cordova-plugin-globalization": "^1.11.0",
    "cordova-plugin-googleplus": "^7.0.2",
    "cordova-plugin-inappbrowser": "^3.0.0",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^2.5.1",
    "cordova-plugin-localization-strings": "git+https://github.com/kelvinhokk/cordova-plugin-localization-strings.git",
    "cordova-plugin-nativestorage": "^2.3.2",
    "cordova-plugin-splashscreen": "^5.0.3",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova-plugin-x-socialsharing": "^5.4.7",
    "cordova-sqlite-storage": "^2.6.0",
    "cordova-support-google-services": "^1.3.1",
    "core-js": "^2.6.9",
    "es6-promise-plugin": "^4.2.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^3.3.0",
    "phonegap-plugin-multidex": "^1.0.0",
